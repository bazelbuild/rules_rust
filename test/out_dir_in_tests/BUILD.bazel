load(
    "//rust:rust.bzl",
    "rust_library",
    "rust_test",
)
load("//cargo:cargo_build_script.bzl", "cargo_build_script")

cargo_build_script(
    name = "build_script",
    srcs = ["build.rs"],
)

rust_library(
    name = "demo_lib",
    srcs = [
        "src/lib.rs",
    ],
    deps = [":build_script"],
)

rust_test(
    name = "demo_lib_test",
    crate = ":demo_lib",
)

