load("@rules_proto//proto:defs.bzl", "proto_library")
load("@rules_rust//rust:defs.bzl", "rust_test")
load("//prost:defs.bzl", "rust_prost_library")

package(default_visibility = ["//prost/private/tests:__subpackages__"])

proto_library(
    name = "b_proto",
    srcs = [
        "b.proto",
    ],
    strip_import_prefix = "/prost/private/tests/transitive_dependencies",
    deps = [
        "//prost/private/tests/transitive_dependencies/b/c:c_proto",
        "@com_google_protobuf//:empty_proto",
    ],
)

rust_prost_library(
    name = "b_rs_proto",
    proto = ":b_proto",
)

rust_test(
    name = "b_test",
    srcs = ["b_test.rs"],
    edition = "2021",
    deps = [
        ":b_rs_proto",
    ],
)
